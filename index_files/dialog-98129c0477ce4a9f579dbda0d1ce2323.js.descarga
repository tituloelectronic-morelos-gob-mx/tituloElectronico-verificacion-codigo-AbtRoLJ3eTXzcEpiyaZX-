function loadingDialog(e){var t=$(e.target).attr("target");if(typeof t!==typeof undefined&&t!==false){}else{var n=$("<div title='Procesando...' style='text-align: center;'><br/>Por favor, espere...</div>");$(n).dialog({modal:true,width:350,height:120,closeOnEscape:false,open:function(e,t){$(".ui-dialog-titlebar-close",t.dialog|t).hide()}})}}function confirmDialog(e){e.preventDefault();var t=$("<div title='Confirmacion...' style='text-align: center;'><br/>Â¿Esta seguro de realizar esta accion?</div>");$(t).dialog({modal:true,width:350,height:180,buttons:{Cancelar:function(){$(this).dialog("close")},Continuar:function(){$(e.target).removeClass("confirm");$(this).dialog("close");if($(e.target).is("a")){window.location=$(e.target).attr("href")}else if($(e.target).is("form")){$(e.target).submit()}loadingDialog(e)}}});$(t).bind("dialogclose",function(e,t){$(".ui-dialog-content").dialog("close")})}function alertDialog(e){var t=$("<div title='Aviso...' style='text-align: center;'><br/>"+e+"</div>");$(t).dialog({modal:true,width:350,height:200,buttons:{Aceptar:function(){$(this).dialog("close")}}});$(t).bind("dialogclose",function(e,t){$(".ui-dialog-content").dialog("close")})}$(document).ready(function(){$(document).keydown(function(e){if(e.keyCode==27)return false});$("a,.showLoad").not(".noLoad").click(function(e){if($(e.target).hasClass("confirm")){confirmDialog(e)}else{loadingDialog(e)}});$("form").submit(function(e){if($(e.target).hasClass("confirm")){confirmDialog(e)}else{loadingDialog(e)}});$(".changeAndSubmit").change(function(e){var t=$(this).closest("form");if(t.hasClass("confirm")){confirmDialog(e)}else{loadingDialog(e)}t.submit()})})